{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "baseUrl": ".",
    "paths": {
      "#/*": [
        "./src/*"
      ],
    },
    "outDir": "dist",
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "target": "es2021",
  },
}